{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "es2015",
    "react",
    "stage-2"
  ],
  "plugins": ["transform-runtime","syntax-dynamic-import",["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }]],
  "env": {
    "test": {
      "presets": ["env", "stage-2"]
    }
  }
}
